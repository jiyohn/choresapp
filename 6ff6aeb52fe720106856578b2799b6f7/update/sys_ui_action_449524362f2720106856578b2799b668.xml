        <script><![CDATA[function AssignValue(){
	var Status = g_form.getValue('status');
	if (Status=='Complete'){
		g_form.showFieldMsg('status',"Unauthorized selection.",'error');
		//g_form.setValue('status','Assigned');
		return false;
	} else if (Status=='Incomplete'){
		g_form.showFieldMsg('status',"Unauthorized selection.",'error');
		//g_form.setValue('status','Assigned');
		return false;
	}else if (Status=='Cancelled'){
		g_form.showFieldMsg('status',"Unauthorized selection.",'error');
		//g_form.setValue('status','Assigned');
		return false;
	}else if (Status=='Verification'){
		g_form.showFieldMsg('status',"Unauthorized selection.",'error');
		//g_form.setValue('status','Assigned');
		return false;
	}
	var answer =confirm("Chore is about to be started.  Click OK to continue.");
	if (answer==true){
	g_form.setValue('status','In progress');
	g_form.submit();
		g_form.save();
	action.setRedirectUrl();
	reloadWindow(window);
	} else {
		g_form.setValue('status','Assigned');
	return false;
}
}
